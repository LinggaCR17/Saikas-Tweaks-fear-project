do
	echo 1 > "$kcal"/kcal_enable
	echo 275 > "$kcal"/kcal_sat
	echo 253 > "$kcal"/kcal_val
	echo 258 > "$kcal"/kcal_cont
	echo 236 238 240 > "$kcal"/kcal
done

